{
  "name": "@react-three/lightmap",
  "version": "0.0.0",
  "description": "In-browser lightmap baker for react-three-fiber and ThreeJS",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/react-three-lightmap.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "potpack": "^1.0.1"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pmndrs/react-three-lightmap.git"
  },
  "author": "Nick Matantsev <nick.matantsev@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pmndrs/react-three-lightmap/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@size-limit/preset-small-lib": "^4.9.2",
    "@storybook/addon-essentials": "^6.1.18",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.1.18",
    "@storybook/addons": "^6.1.18",
    "@storybook/react": "^6.1.18",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "babel-loader": "^8.2.2",
    "husky": "^5.0.9",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-three-fiber": "^5.3.1",
    "size-limit": "^4.9.2",
    "three": "^0.121.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-three-fiber": ">=5.3.0",
    "three": ">=0.121.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "size-limit": [
    {
      "path": "dist/react-three-lightmap.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-three-lightmap.esm.js",
      "limit": "10 KB"
    }
  ]
}
